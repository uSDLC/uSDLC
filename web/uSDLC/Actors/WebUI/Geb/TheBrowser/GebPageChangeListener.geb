class EchoingPageChangeListener implements geb.PageChangeListener {
    void pageWillChange(geb.Browser browser, geb.Page oldPage, geb.Page newPage) {
        EchoingPageChangeListener.count++
    }

    static count = 0
}
browse UsdlcPage
browser.registerPageChangeListener(new EchoingPageChangeListener())
EchoingPageChangeListener.count = 0
browser.to ScratchPage
assert EchoingPageChangeListener.count == 1
