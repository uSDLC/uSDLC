browse UsdlcPage
waitFor { title }
firstTitle = title
// go to base URL (same page as above in uSDLC
browser.go()
assert firstTitle == title
// go to the root page (a uSDLC special)
browser.go "/?frontPage.html"
assert title == "Unifying the Software Development Life-cycle"
// use parameters
browser.go "/root", action: "raw"
assert title == ""
