// browse to the root and check that it has a viable title
browse UsdlcPage
assert page instanceof UsdlcPage
assert title != "Title"

// define a page and prove that the page reference is set
class SecondPage extends UsdlcPage {
	static url = "/Scratch/SecondPage"
}
page(SecondPage)
assert page instanceof SecondPage

// Finally check that the 'real' page hasn't moved
assert title != "Title"
