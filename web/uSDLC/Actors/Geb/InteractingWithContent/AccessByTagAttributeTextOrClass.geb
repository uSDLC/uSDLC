assert $("p.ta").text() == "a"
assert $("p.ta").tag() == "p"
assert $("p.ta").@title == "a"
assert $("p.ta").classes() == ['ta', 'para'] as HashSet

assert $("p.para")*.text() == ["a", "b", "v"]
assert $("p.para")*.tag() == ["p", "p", "p"]
assert $("p.para")*.@title == ["a", "b", "c"]
assert $("p.para")*.classes() == [
		["ta", "para"] as HashSet,
		["tb", "para"] as HashSet,
		["tc", "para"] as HashSet
]
