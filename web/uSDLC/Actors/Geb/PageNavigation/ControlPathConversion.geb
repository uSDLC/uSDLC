class Language {
	String ext
	String name
}
class ActorPage extends UsdlcPage {
	static url = "/uSDLC/Actors"

	def convertToPath(Language language) {
		language.name
	}
}

def groovy = new Language(ext: 'groovy', name: 'Groovy')

browse UsdlcPage
to ActorPage, groovy
assert title == "Groovy Actor"
