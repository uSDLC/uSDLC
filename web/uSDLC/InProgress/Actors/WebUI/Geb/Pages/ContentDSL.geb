class ContentDslExample extends UsdlcPage {
	static url = "/uSDLC/Actors/Geb/Pages"
	static content = {
		theDiv { $('div', id: 'contentA') }
	}
}
browse ContentDslExample

assert theDiv.text() == 'contentA'
// delegated from page
assert page.theDiv.text() == 'contentA'
// defined content can be property or method
assert theDiv().text() == 'contentA'
