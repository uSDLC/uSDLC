if not window.usdlc
  url = window.location
  if url.pathname isnt '/'
    document.cookie = "currentPage=#{url.pathname}"
  url.href = "#{url.protocol}//#{url.host}/usdlc/home"
else
  synopsis = $('div.section:first')
  children = synopsis.children().not('div.pageType')
  if children.size() == 0
    usdlc.form 'Synopsis:', usdlc.selectSynopsisTab
